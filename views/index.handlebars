<!-- 5. Output the animalType string, pet boolean, and fierceness number to the page for all the pets. -->
<ul>
  {{#each anims}}
  <h1>[An] Animal</h1>
    <li>
      <p>Species: {{animalType}}</p>
      <p>Pet: {{pet}}</p>
      <p>Fierceness: {{fierceness}}</p>
    </li>
  {{/each}}
</ul>
